{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: g++-9 arquivo de build ativo",
      "command": "/usr/bin/g++-9",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${file}",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": "build",
      "detail": "Tarefa gerada pelo Depurador."
    },
    {
      "type": "cppbuild",
      "label": "C/C++: g++-10 arquivo de build ativo",
      "command": "/usr/bin/g++-10",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${file}",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}",
        "-lm"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": "build",
      "detail": "Tarefa gerada pelo Depurador."
    }
  ],
  "version": "2.0.0"
}